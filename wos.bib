@article{ WOS:000310315900005,
Author = {Liu, Zheng and Tesfamariam, Solomon},
Title = {Prediction of lateral spread displacement: data-driven approaches},
Journal = {BULLETIN OF EARTHQUAKE ENGINEERING},
