@article{ WOS:000420229200006,
Author = {Jirdehi, Reza Ahmadi and Mamoudan, Hamidreza Talebi and Sarkaleh,
   Hossein Hassanpanah},
Title = {Applying GMDH-type Neural Network and Particle warm Optimization for
   Prediction of Liquefaction Induced Lateral Displacements},
