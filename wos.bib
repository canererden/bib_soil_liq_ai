Title = {Determination of liquefaction susceptibility of soil: a least square
   support vector machine approach},
Journal = {INTERNATIONAL JOURNAL FOR NUMERICAL AND ANALYTICAL METHODS IN
   GEOMECHANICS},
Year = {2013},
Volume = {37},
