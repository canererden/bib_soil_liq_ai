@inproceedings{ WOS:000221357500041,
Author = {Chen, BS and Bailey, MJ},
Editor = {Turner, JP and Mayne, PW},
Title = {Lessons learned from a stone column test program in glacial deposits},
Booktitle = {GEO-SUPPORT 2004: DRILLED SHAFTS, MICROPILING, DEEP MIXING, REMEDIAL
   METHODS, AND SPECIALTY FOUNDATION SYSTEMS},
