@inproceedings{ WOS:000075967300042,
Author = {Najjar, YM and Ali, HE},
Editor = {Dakoulas, P and Yegian, M and Holtz, B},
Title = {CPT-based liquefaction potential assessment: A neuronet approach},
Booktitle = {GEOTECHNICAL EARTHQUAKE ENGINEERING AND SOIL DYNAMICS III, VOL 1 AND 2},
Series = {GEOTECHNICAL SPECIAL PUBLICATION},
Year = {1998},
Number = {75},
Pages = {542-553},
Note = {Geo-Institute Specialty Conference on Geotechnical Earthquake
   Engineering and Soil Dynamics, UNIV WASHINGTON, SEATTLE, WA, AUG 03-06,
   1998},
Abstract = {A back-propagation Artificial Neural Network (ANN) approach was used as
   a new technique to characterize the soil liquefaction resistance
   utilizing more than 170 field data sets representing various earthquake
   sites from around the world. The two most promising and accurate
   neuronet-based models are presented in this paper and their
   classification accuracy is compared with results obtained using other
   approaches. Furthermore, the accuracy of the designed networks was
   validated against additional data sets never used before in network
   training or testing phases. ANN Model \#2 was further deduce to produce
   a simple and accurate liquefaction potential assessment chart that can
   be easily used by geotechnical engineers in liquefaction assessment
   tasks. Overall, ANN approach outperformed all other approaches
   investigated in this study by accurately assessing the soil liquefaction
   resistance.},
