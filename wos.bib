@article{ WOS:000591902000007,
Author = {Xie, Yazhou and Ebad Sichani, Majid and Padgett, Jamie E. and DesRoches,
   Reginald},
Title = {The promise of implementing machine learning in earthquake engineering:
   A state-of-the-art review},
Journal = {EARTHQUAKE SPECTRA},
