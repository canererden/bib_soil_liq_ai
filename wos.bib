@article{ WOS:000580209800001,
Author = {Chen, Zhixiong and Li, Hongrui and Goh, Anthony Teck Chee and Wu,
   Chongzhi and Zhang, Wengang},
Title = {Soil Liquefaction Assessment Using Soft Computing Approaches Based on
   Capacity Energy Concept},
Journal = {GEOSCIENCES},
