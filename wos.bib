@article{ WOS:000299177300019,
Author = {Gandomi, Amir Hossein and Alavi, Amir Hossein},
Title = {A new multi-gene genetic programming approach to non-linear system
   modeling. Part II: geotechnical and earthquake engineering problems},
Journal = {NEURAL COMPUTING \& APPLICATIONS},
