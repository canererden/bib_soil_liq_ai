@article{ WOS:000471237400042,
Author = {Avval, Yasaman Jafari and Derakhshani, Ali},
Title = {New formulas for predicting liquefaction-induced lateral spreading:
   model tree approach},
Journal = {BULLETIN OF ENGINEERING GEOLOGY AND THE ENVIRONMENT},
