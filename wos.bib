@article{ WOS:000288053800026,
Author = {Farrokhzad, Farzad and Choobbasti, Asskar Janalizadeh and Barari, Amin
   and Ibsen, Lars Bo},
Title = {ASSESSING LANDSLIDE HAZARD USING ARTIFICIAL NEURAL NETWORK: CASE STUDY
   OF MAZANDARAN, IRAN},
Journal = {CARPATHIAN JOURNAL OF EARTH AND ENVIRONMENTAL SCIENCES},
